{
  "name": "nucampsite",
  "version": "1.0.0",
  "description": "This is a website for the fictional campsites review site NuCamp.",
  "main": "index.html",
  "scripts": {
    "lite": "lite-server",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "sass css:css",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'"
  },
  "author": "Kristine Johnson",
  "license": "ISC",
  "devDependencies": {
    "lite-server": "^2.2.0",
    "onchange": "^7.0.0",
    "parallelshell": "3.0.1",
    "sass": "^1.56.2"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristinealli/NuCampCampSite.git"
  },
  "bugs": {
    "url": "https://github.com/kristinealli/NuCampCampSite/issues"
  },
  "homepage": "https://github.com/kristinealli/NuCampCampSite#readme"
